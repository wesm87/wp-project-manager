{
  presets: [
    'es2016',
    'es2017',
  ],
  plugins: [
    'transform-runtime',
    'transform-flow-comments',
    'transform-decorators-legacy',
    'transform-class-properties',
    'transform-object-rest-spread',
    'transform-regenerator',
  ],
  env: {
    development: {
      presets: [
        'es2015',
      ],
    },
    production: {
      presets: [
        ['es2015', { modules: false }],
      ],
    },
  },
}
